{
  "name": "unplugin-hot-export",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@7.1.5",
  "description": "",
  "author": "sudongyuer",
  "license": "MIT",
  "funding": "https://github.com/sponsors/sudongyuer",
  "homepage": "https://github.com/sudongyuer/unplugin-hot-export#readme",
  "repository": "https://github.com/sudongyuer/unplugin-hot-export",
  "bugs": "https://github.com/sudongyuer/unplugin-hot-export/issues",
  "keywords": [
    "vite",
    "vite-plugin",
    "unplugin-hot-export"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "autoexport": "./node_modules/.bin/autoexport"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub --declaration",
    "build": "unbuild",
    "test": "vitest",
    "version": "changeset version",
    "release": "pnpm run build && pnpm run release:only",
    "release:only": "changeset publish",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --cache",
    "lint-fix": "eslint . --fix",
    "try": "tsx src/index.ts"
  },
  "dependencies": {
    "auto-export": "^0.3.3",
    "chokidar": "^3.5.3",
    "unconfig": "^0.3.5",
    "unplugin": "^0.8.0"
  },
  "devDependencies": {
    "@sudongyuer/eslint-config": "^0.1.3",
    "@types/node": "^18.0.0",
    "eslint": "^8.18.0",
    "pnpm": "^7.3.0",
    "tsx": "^3.6.0",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.2"
  },
  "eslintConfig": {
    "extends": [
      "@sudongyuer"
    ]
  }
}
